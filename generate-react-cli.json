{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withIndex": true,
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "componentTemplates/default/TemplateName.tsx",
        "test": "componentTemplates/default/TemplateName.test.tsx",
        "style": "componentTemplates/default/TemplateName.module.scss",
        "index": "componentTemplates/default/index.ts"
      }
    },
    "container": {
      "path": "src/components/containers",
      "withIndex": true,
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "componentTemplates/container/TemplateName.tsx",
        "index": "componentTemplates/default/index.ts"
      }
    },
    "smart": {
      "path": "src/components/smart",
      "withIndex": true,
      "withStyle": false,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "componentTemplates/smart/TemplateName.tsx",
        "test": "componentTemplates/default/TemplateName.test.tsx",
        "index": "componentTemplates/default/index.ts"
      }
    },
    "simple": {
      "path": "src/components/simple",
      "withIndex": true,
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "componentTemplates/simple/TemplateName.tsx",
        "index": "componentTemplates/default/index.ts"
      }
    },
    "ui": {
      "path": "src/components/ui",
      "withIndex": true,
      "withStyle": false,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "componentTemplates/smart/TemplateName.tsx",
        "test": "componentTemplates/default/TemplateName.test.tsx",
        "index": "componentTemplates/default/index.ts"
      }
    }
  }
}
